{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"],
        "@": ["src/*"],
        "core": ["src/core/*"],
        "components": ["src/components/*"],
        "modules": ["src/modules/*"],
        "router": ["src/router/*"],
        "store": ["src/store/*"],
        "assets": ["src/assets/*"],
        "fonts": ["src/assets/fonts/*"],
        "images": ["src/assets/images/*"]
      }
    },
    "exclude": ["node_modules", "dist"]
  }
  